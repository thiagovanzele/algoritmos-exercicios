package leetcode.exercise.Ex007_Fibonacci;

public class Ex007 {

	public static void main(String[] args) {

		mostraFibonacci(5);
	}

	private static void mostraFibonacci(int n) {
		int a = 0, b = 1;
		
		for (int i = 0; i <= n; i++) {
			System.out.println(a);
			
			int proximo = a + b;
			a = b;
			b = proximo;
			
			
		}
		
	}
	

}
