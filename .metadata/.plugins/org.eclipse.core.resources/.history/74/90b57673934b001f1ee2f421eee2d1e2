package leetcode.exercise.Ex001_TwoSum;

public class testeeee {

	public static void main(String[] args) {
		
		int[][] nums = new int[3][3];
		int[][] nums2 = new int[3][3];
		
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 3; j++) {
				nums[i][j] = 2;
			}
		}
		
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 3; j++) {
				nums2[i][j] = 3;
			}
		}
		
		for (int[] n : nums) {
			for (int num : n) {
				System.out.print(num + " ");
			}
			System.out.println();
		}
		
		System.out.println("====================");
		
		for (int[] n : nums2) {
			for (int num : n) {
				System.out.print(num + " ");
			}
			System.out.println();
		}

		
	}

}
