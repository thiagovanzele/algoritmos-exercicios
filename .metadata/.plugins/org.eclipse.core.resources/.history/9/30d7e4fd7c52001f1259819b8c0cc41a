package leetcode.exercise.Ex002_RemoveDuplicates;

import java.util.Set;
import java.util.TreeSet;

public class Ex002 {

	public static void main(String[] args) {
		int[] numeros = new int[] {0,0,1,1,1,2,2,3,3,4};
		System.out.println(removeDuplicates(numeros));
	}
	
	public static int removeDuplicates(int[] nums) {
		Set<Integer> numerosUnicos = new TreeSet<Integer>();
		for (int n : nums) {
			numerosUnicos.add(n);
		}
		
		return numerosUnicos.size();
	}

}
